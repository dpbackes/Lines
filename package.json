{
    "name": "lines",
    "displayName": "Lines",
    "description": "Perform lines based operations. (mark, delete, etc.)",
    "version": "0.0.1",
    "publisher": "DanBackes",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.linesRemove"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.linesRemove",
            "title": "lines remove"
        }]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^1.8.5",
        "vscode": "^0.11.0"
    }
}